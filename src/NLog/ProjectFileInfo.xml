     </ItemGroup>
   </Project>

   <Project File="NLog.Extended.doc.csproj">
     <ItemGroup Name="Compile">
        <FileSet Include="ExtendedProfileOnly" />
        <FileSet Include="CommonFiles" />
     </ItemGroup>
   </Project>
</ProjectFiles>